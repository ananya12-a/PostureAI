AAuBC,GAAvB,EAA2B;AACvB,kBAAQ,SAACA,GAAG,CAAC,GAAD,CAAH,GAASD,GAAG,CAAC,GAAD,CAAb,EAAqB,CAArB,aAA0BC,GAAG,CAAC,GAAD,CAAH,GAASD,GAAG,CAAC,GAAD,CAAtC,EAA8C,CAA9C,CAAR,EAA0D,GAA1D;AACH;;AACD,SAASgC,uBAAT,CAAiChC,GAAjC,EAAqCC,GAArC,EAAyCgC,GAAzC,EAA6C;AACrC;AACA,MAAI/B,EAAE,GAACF,GAAG,CAAC,GAAD,CAAV;AACA,MAAIG,EAAE,GAACF,GAAG,CAAC,GAAD,CAAV;AACA,MAAIiC,EAAE,GAACD,GAAG,CAAC,GAAD,CAAV;AACA,MAAI7B,EAAE,GAACJ,GAAG,CAAC,GAAD,CAAV;AACA,MAAIK,EAAE,GAACJ,GAAG,CAAC,GAAD,CAAV;AACA,MAAIkC,EAAE,GAACF,GAAG,CAAC,GAAD,CAAV;AACA,MAAIG,MAAM,YAAI,SAACjC,EAAE,GAACD,EAAJ,EAAS,CAAT,aAAcG,EAAE,GAACD,EAAjB,EAAsB,CAAtB,CAAJ,EAA8B,GAA9B,CAAV;AACA,MAAIiC,MAAM,YAAI,SAAClC,EAAE,GAAC+B,EAAJ,EAAS,CAAT,aAAc7B,EAAE,GAAC8B,EAAjB,EAAsB,CAAtB,CAAJ,EAA8B,GAA9B,CAAV;AACA,MAAIG,MAAM,YAAI,SAACJ,EAAE,GAAChC,EAAJ,EAAS,CAAT,aAAciC,EAAE,GAAC/B,EAAjB,EAAsB,CAAtB,CAAJ,EAA8B,GAA9B,CAAV;AACA,SAAOQ,IAAI,CAAC2B,IAAL,CAAU,CAAC,SAAAF,MAAM,EAAE,CAAF,CAAN,YAAUD,MAAV,EAAkB,CAAlB,aAAoBE,MAApB,EAA4B,CAA5B,CAAD,KAAiC,IAAED,MAAF,GAASD,MAA1C,CAAV,IAA+D,GAA/D,GAAmExB,IAAI,CAACI,EAA/E;AACP;;AAED,SAAQpB,OAAR,EAAiBa,SAAjB,EAA4BK,WAA5B,EAAyCf,gBAAzC,EAA2DkB,WAA3D,EAAwEC,cAAxE,EAAwFM,aAAxF,EAAuGM,WAAvG,EAAoHH,WAApH,EAAgIK,uBAAhI,EAAyJD,QAAzJ","sourcesContent":["\n\nfunction average(a,b){\n    return (a+b)/2\n}\n\n\nfunction averageKeypoints(kp1, kp2) {\n    let x1=kp1['x']\n    let x2=kp2['x']\n    let y1=kp1['y']\n    let y2=kp2['y']\n    return {x: (x1+x2)/2, y:(y1+y2)/2, z:(kp1['z']+kp2['z'])/2}\n}\n\nfunction calcAngle(opp, adj){\n    return Math.atan(opp/adj)\n}\n\nfunction angleToHori(kp1,kp2){\n    let x1=kp1['x']\n    let x2=kp2['x']\n    let y1=kp1['y']\n    let y2=kp2['y']\n    \n    //console.log(\"Angle To Hori:\", kp1, kp2, x1, x2, y1, y2);\n    return (Math.atan(Math.abs(y2-y1)/Math.abs(x2-x1)))* 180 / Math.PI\n}\n\nfunction angleToVert(kp1,kp2){\n    let x1=kp1['x']\n    let x2=kp2['x']\n    let y1=kp1['y']\n    let y2=kp2['y']\n    return (Math.atan(Math.abs(x2-x1)/Math.abs(y2-y1)))* 180 / Math.PI\n}\n\nfunction angleScoreCalc(angle, idealAngle, halfRange){\n    let diff = 10/halfRange * Math.abs(idealAngle - angle)\n    return Math.round((idealAngle-diff)/idealAngle * 100)\n}\nfunction distScoreCalc(dist, refDist){\n    return Math.round((refDist-dist)/refDist * 100)\n}\nfunction gtScoreCalc(val1, val2){\n    if (val2>val1) {\n        if (val2-val1<val2) {\n            return Math.round((val2 - Math.abs(val1-val2))/val2 * 100,0);\n        }\n        else {\n            return Math.round((val2-val1)/(val1+val2) * 100,0);\n        }\n    }\n    else return 100.0\n}\nfunction ltScoreCalc(val1, val2){\n    if (val2<val1){\n        if (val1-val2<val2) return Math.round((val2 - Math.abs(val1-val2))/val2 * 100,0)\n        else return Math.round((val1-val2)/(val1+val2) * 100,0)\n    }\n    else return 100.0\n}\n\nfunction distance(kp1, kp2){\n    return ((kp2['x']-kp1['x'])**2 + (kp2['y']-kp1['y'])**2)**0.5\n}\nfunction angleBetweenThreePoints(kp1,kp2,kp3){\n        //finds angle at middle point\n        let x1=kp1['x']\n        let x2=kp2['x']\n        let x3=kp3['x']\n        let y1=kp1['y']\n        let y2=kp2['y']\n        let y3=kp3['y']\n        let dist12 = ((x2-x1)**2 + (y2-y1)**2)**0.5\n        let dist23 = ((x2-x3)**2 + (y2-y3)**2)**0.5\n        let dist13 = ((x3-x1)**2 + (y3-y1)**2)**0.5\n        return Math.acos((dist23**2+dist12**2-dist13**2)/(2*dist23*dist12)) * 180/Math.PI\n}\n\nexport {average, calcAngle, angleToHori, averageKeypoints, angleToVert, angleScoreCalc, distScoreCalc, ltScoreCalc, gtScoreCalc,angleBetweenThreePoints, distance}"]}]}